{
  "name": "cra-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write --loglevel warn",
    "stylelint:check": "stylelint \"**/*.scss\" --max-warnings 0",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix --max-warnings 0",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix --quiet",
    "lint:check": "npm run stylelint:check && npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run stylelint:fix && npm run eslint:fix && npm run prettier:fix"
  },
  "lint-staged": {
    "*": "prettier --write --loglevel warn",
    "*.{css,scss}": "stylelint --fix --max-warnings 0",
    "*.{js,jsx,ts,tsx}": "eslint --fix --quiet"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.2",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/node": "16.11.41",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "postcss": "8.4.14",
    "postcss-scss": "4.0.4",
    "prettier": "2.7.1",
    "sass": "1.53.0",
    "stylelint": "14.9.1",
    "stylelint-config-hudochenkov": "7.0.1",
    "stylelint-config-standard": "26.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.2.0",
    "typescript": "4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
